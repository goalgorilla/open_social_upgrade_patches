    "type": "metapackage",
    "license": "GPLv2",
    "minimum-stability": "dev",
    "require": {
        "drupal/dynamic_entity_reference": "^1"
    },
    "extra": {
        "patches": {
            "drupal/dynamic_entity_reference": {
                "Issue #3099176: Errors when new entity types are added (in certain cases)": "https://www.drupal.org/files/issues/2022-02-14/3099176-1.x-14.patch"
            }
        }
    }
}
